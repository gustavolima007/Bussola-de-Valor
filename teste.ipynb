   "source": [
    "import yfinance as yf\n",
    "\n",
    "ticker = yf.Ticker(\"PETR4.SA\")\n",
    "hist = ticker.history(period=\"5y\")\n",
    "inicio = hist[\"Close\"].iloc[0]\n",
    "fim = hist[\"Close\"].iloc[-1]\n",
    "anos = 5\n",
    "cagr = ((fim / inicio) ** (1 / anos)) - 1\n",
    "print(f\"CAGR: {cagr:.2%}\")"
   ]
  }
 ],
